{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "EX LIBRIS",
  "description": "EX LIBRIS. English version",
  "type": "object",
  "properties": {
    "description": {
      "description": "Description",
      "type": "string"
    },
    "height": {
      "description": "Height (mm)",
      "type": "string"
    },
    "width": {
      "description": "Width (mm)",
      "type": "string"
    },
    "art_medium": {
      "description": "Art Medium",
      "type": "string"
    },
    "artwork_surface": {
      "description": "Artwork Surface",
      "type": "string"
    },
    "date_created": {
      "description": "Date Created",
      "type": "string",
      "format": "date"
    },
    "digital_assets": {
      "description": "Digital Assets",
      "type": "object",
      "properties": {
        "primary_image": {
          "description": "Primary Image",
          "type": "string",
          "format": "uuid"
        },
        "picture_back": {
          "description": "Picture Back",
          "type": "string",
          "format": "uuid"
        },
        "picture_front_without_frame": {
          "description": "Picture Front Without Frame",
          "type": "string",
          "format": "uuid"
        }
      },
      "required": [
        "primary_image"
      ]
    }
  },
  "required": [
    "description",
    "height",
    "width",
    "date_created",
    "digital_assets"
  ]
}